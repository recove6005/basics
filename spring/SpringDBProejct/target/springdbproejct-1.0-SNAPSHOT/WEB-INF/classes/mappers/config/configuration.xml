<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="defaultFetchSize" value="100"/>
        <setting name="defaultStatementTimeout" value="25"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="BoardDTO" type="com.example.springdbproejct.dtos.BoardDTO"/>
        <typeAlias alias="ClubDTO" type="com.example.springdbproejct.dtos.ClubDTO" />
        <typeAlias alias="FileDTO" type="com.example.springdbproejct.dtos.FileDTO" />
        <typeAlias alias="RoomDTO" type="com.example.springdbproejct.dtos.RoomDTO" />
        <typeAlias alias="StdClubDTO" type="com.example.springdbproejct.dtos.StdClubDTO" />
        <typeAlias alias="StdDTO" type="com.example.springdbproejct.dtos.StdDTO" />
        <typeAlias alias="UserDTO" type="com.example.springdbproejct.dtos.UserDTO" />
        <typeAlias alias="FileVO" type="com.example.springdbproejct.vos.FileVO" />
        <typeAlias alias="CommentDTO" type="com.example.springdbproejct.dtos.CommentDTO" />
    </typeAliases>
</configuration>